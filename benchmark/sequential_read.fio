[global]
bs=256k
runtime=30s
unit_base=1 # Kbps
time_based
group_reporting
thread

[sequential_read]
ramp_time=10s # Avoid any chance of cold S3 influencing the result
size=${SIZE_GIB}Gi
; filename=${SIZE_GIB}GiB.bin
filename_format=j$jobnum_${SIZE_GIB}GiB.bin
rw=read
ioengine=sync
fallocate=none
numjobs=${NUMJOBS}
openfiles=${NUMJOBS}
direct=${DIRECT}
